/*eslint linebreak-style: ["error", "windows"]*/
{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": ["error", "windows"],
        "semi":"off",
        "object-curly-newline":"off",
        "operator-linebreak":"off",
        "function-paren-newline":"off"
    }
}
